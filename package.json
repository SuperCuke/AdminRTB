{
  "name": "typescript-react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development node dev_server.js",
    "postinstall": "npm run typings && npm run build",
    "typings": "rimraf typings/ && typings install"
  },
  "dependencies": {
    "react-bootstrap": "^0.30.3",
    "react-bootstrap-table": "^2.5.9"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.0.0",
    "babel-core": "^6.9.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-bootstrap-table": "^2.5.8",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.4",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.4.0",
    "redux-form": "^5.3.2",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  }
}
